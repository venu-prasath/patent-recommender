{
    "patent_id": "US-11259800-B2",
    "title": "Retraction mechanism with clutch-less drive for use with surgical apparatus ",
    "assignee": "Covidien Lp",
    "publication_date": "2022-03-01",
    "patent_link": "https://patents.google.com/patent/US11259800B2/en",
